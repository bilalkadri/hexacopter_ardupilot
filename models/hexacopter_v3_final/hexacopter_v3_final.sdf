<?xml version='1.0'?>

<sdf version='1.6'>

  <model name='hexacopter_v3_final'>
    <pose>0 0 0 0 0 0</pose>

    <!-- Hexacopter Base / Body / Frame -->
    <link name='hexa_base_link'>
      
      <velocity_decay>
        <linear>0.0</linear>
        <angular>0.0</angular>
      </velocity_decay>
     
      <inertial>
        <pose>0.062869 -0.07024 -0.0157698 0 0 0</pose>
        <mass>0.48936</mass>
        <inertia>
          <ixx>0.00337831</ixx>
          <ixy>-7.63523e-07</ixy>
          <ixz>7.28723e-06</ixz>
          <iyy>0.00448006</iyy>
          <iyz>-4.79159e-08</iyz>
          <izz>0.00131736</izz>
        </inertia>
      </inertial>
   
      <collision name='hexa_base_collision'>
        <pose>0 0 0 0 -0 0</pose>
        <max_contacts>10</max_contacts>
        <geometry>
          <mesh>
            <uri>model://hexacopter_v3_final/meshes/base_link.STL </uri>
          </mesh>
        </geometry>
        <surface>
          <contact>
            <ode>
              <kp>1000000000000.0</kp>
              <soft_erp>0.2</soft_erp>
              <kd>1</kd>
              <min_depth>0</min_depth>
              <soft_cfm>0</soft_cfm>
              <max_vel>0.01</max_vel>
            </ode>
          </contact>
          <friction>
            <ode>
              <mu>1.0</mu>
              <mu2>1.0</mu2>
            </ode>
          </friction>
        </surface>  
      </collision>

      <visual name='hexa_base_visual'>
        <!--<pose frame=''>0 0 0 0 -0 0</pose>-->
        <geometry>
          <mesh>
            <uri>model://hexacopter_v3_final/meshes/base_link.STL </uri>
          </mesh>
        </geometry>
        <material>
           <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/DarkGrey</name>
          </script>
        </material> 
      </visual>
    </link>

    <!-- Hexacopter IMU Sensors -->
    <link name='hexacopter/ground_truth/odometry_sensorgt_link'>
      <pose>0 0 0 0 -0 0</pose>
      <inertial>
        <pose>0 0 0 0 -0 0</pose>
        <mass>0.15</mass>
        <inertia>
          <ixx>0.0001</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.0002</iyy>
          <iyz>0</iyz>
          <izz>0.0002</izz>
        </inertia>
      </inertial>
    </link>
    <joint name='hexacopter/ground_truth/odometry_sensorgt_joint' type='revolute'>
      <child>hexacopter/ground_truth/odometry_sensorgt_link</child>
      <parent>base_link</parent>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>0</lower>
          <upper>0</upper>
          <effort>0</effort>
          <velocity>0</velocity>
        </limit>
        <dynamics>
          <damping>1.0</damping>
        </dynamics>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
      <physics>
        <ode>
          <implicit_spring_damper>1</implicit_spring_damper>
        </ode>
      </physics>
    </joint>

    <link name='hexacopter/imu_link'>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.15</mass>
        <inertia>
          <ixx>0.00001</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.00002</iyy>
          <iyz>0</iyz>
          <izz>0.00002</izz>
        </inertia>
      </inertial>
      <sensor name="imu_sensor" type="imu">
        <pose>0 0 0 3.141593 0 0</pose>
        <always_on>1</always_on>
        <update_rate>1000.0</update_rate>
      </sensor>
    </link>
    <joint name='hexacopter/imu_joint' type='revolute'>
      <child>hexacopter/imu_link</child>
      <parent>base_link</parent>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>0</lower>
          <upper>0</upper>
          <effort>0</effort>
          <velocity>0</velocity>
        </limit>
        <dynamics>
          <damping>1.0</damping>
        </dynamics>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
      <physics>
        <ode>
          <implicit_spring_damper>1</implicit_spring_damper>
        </ode>
      </physics>
    </joint>

    <!-- Hexacopter Propeller Definitions -->

    <link name='Prop_1'>
      <pose>0.39429 -0.15764 0.14958 0 -0 0</pose>
      <inertial>
        <pose frame=''>2e-06 0 -0.005964 0 -0 0</pose>
        <mass>0.0895254</mass>
        <inertia>
          <ixx>3.712e-07</ixx>
          <ixy>4.33806e-07</ixy>
          <ixz>5.74428e-11</ixz>
          <iyy>3.99344e-05</iyy>
          <iyz>-7.00107e-13</iyz>
          <izz>4.02239e-05</izz>
        </inertia>
      </inertial>
      <self_collide>0</self_collide>
      <enable_wind>0</enable_wind>
      <kinematic>0</kinematic>
      <gravity>1</gravity>
      <velocity_decay/>
      <visual name='Prop_1_visual'>
        <pose frame=''>0 0 0 0 -0 0</pose>
        <geometry>
          <mesh>
            <scale>1 1 1</scale>
            <uri>model://hexacopter_v3_final/meshes/iris_prop_cw.dae</uri>          
          </mesh>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Indigo</name>
            <uri>__default__</uri>
          </script>
        </material>
        <transparency>0</transparency>
        <cast_shadows>1</cast_shadows>
      </visual>
      <collision name='Prop_1_collision'>
        <laser_retro>0</laser_retro>
        <max_contacts>10</max_contacts>
        <pose frame=''>0 0 0 0 -0 0</pose>
        <geometry>
          <mesh>
            <uri>model://hexacopter_v3_final/meshes/iris_prop_cw.dae</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>1</mu>
              <mu2>1</mu2>
              <fdir1>0 0 0</fdir1>
              <slip1>0</slip1>
              <slip2>0</slip2>
            </ode>
            <torsional>
              <coefficient>1</coefficient>
              <patch_radius>0</patch_radius>
              <surface_radius>0</surface_radius>
              <use_patch_radius>1</use_patch_radius>
              <ode>
                <slip>0</slip>
              </ode>
            </torsional>
          </friction>
          <bounce>
            <restitution_coefficient>0</restitution_coefficient>
            <threshold>1e+06</threshold>
          </bounce>
          <contact>
            <collide_without_contact>0</collide_without_contact>
            <collide_without_contact_bitmask>1</collide_without_contact_bitmask>
            <collide_bitmask>1</collide_bitmask>
            <ode>
              <soft_cfm>0</soft_cfm>
              <soft_erp>0.2</soft_erp>
              <kp>1e+13</kp>
              <kd>1</kd>
              <max_vel>0.01</max_vel>
              <min_depth>0</min_depth>
            </ode>
            <bullet>
              <split_impulse>1</split_impulse>
              <split_impulse_penetration_threshold>-0.01</split_impulse_penetration_threshold>
              <soft_cfm>0</soft_cfm>
              <soft_erp>0.2</soft_erp>
              <kp>1e+13</kp>
              <kd>1</kd>
            </bullet>
          </contact>
        </surface>
      </collision>
    </link>


    <joint name='Prop_1_joint' type='revolute'>
        <parent>base_link</parent>
        <child>Prop_1</child>
        <pose frame=''>0 0 0 0 -0 0</pose>
        <axis>
          <xyz>0 0 1</xyz>
          <use_parent_model_frame>1</use_parent_model_frame>
          <limit>
            <lower>-1e+16</lower>
            <upper>1e+16</upper>
          </limit>
          <dynamics>
            <spring_reference>0</spring_reference>
            <spring_stiffness>0</spring_stiffness>
            <damping>0</damping>
            <friction>0</friction>
          </dynamics>
        </axis>
        <physics>
          <ode>
          <implicit_spring_damper>1</implicit_spring_damper>
            <limit>
              <cfm>0</cfm>
              <erp>0.2</erp>
            </limit>
            <suspension>
              <cfm>0</cfm>
              <erp>0.2</erp>
            </suspension>
          </ode>
        </physics>
    </joint>

    

  </model>
</sdf>
